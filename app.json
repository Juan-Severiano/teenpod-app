{
  "expo": {
    "name": "TeenPod",
    "slug": "podcast-gleysin",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/icon-tp.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-tp.png", 
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "plugins": [
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-tp.png",
        "backgroundColor": "#000000"
      },
      "package": "com.juansev.teenpod",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon-tp.png"
    },
    "extra": {
      "eas": {
        "projectId": "126a3512-10fd-48ca-9ce3-5389509b1bfe"
      }
    }
  }
}
